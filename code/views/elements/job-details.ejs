<%- include('./job-details-tag',{job:job}) %>
<div class="single-content2 py-4">
    <h4>Mô tả:</h4>
    <div class="company-description">
        <p> <%- job.description %></p>
    </div>
</div>
<div class="single-content3 py-4">
    <h4>Nơi làm việc:</h4>
    <p> <%- job.location %></p>
</div>
<div class="single-content4 py-4">
    <h4>Số lượng tuyển:</h4>
    <span class="ml-4"><%- job.amount %></span>
    <h4 class="mt-4">Loại:</h4>
    <span class="ml-4"><%- job.type %></span>
    <h4 class="mt-4">Cấp bậc:</h4>
    <span class="ml-4"><%- job.level %></span>
</div>
<div class="single-content5 py-4">
    <h4>Yêu cầu chi tiết:</h4>
    <%- job.requirement %>
</div>